<!-- Scaffolding generated by Members <%= members_get_full_version_string %> -->

<h2><%= plural_name.humanize.capitalize %><%%= members_pagination_details @<%= plural_name %> %></h2>

<%%= render :partial => 'members/<%= plural_name %>/table', :locals => { :<%= plural_name %> => @<%= plural_name %> } %>

<div class="text-center">
	<%%= will_paginate @<%= plural_name %> %>
</div>

<%%= content_for :sidebar do %>
<% unless @read_only %>
	<li><%%= link_to "#{members_show_icon "plus-sign"}Add a new <%= singular_name.humanize.downcase %>".html_safe, new_members_<%= singular_name %>_path %></li>
<% end %>
<%% end %>